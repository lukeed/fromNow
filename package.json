{
  "name": "fromnow",
  "version": "3.0.1",
  "repository": "lukeed/fromnow",
  "description": "A tiny (339B) utility for human-readable time differences between now and past or future dates.",
  "unpkg": "dist/fromnow.min.js",
  "module": "dist/fromnow.mjs",
  "main": "dist/fromnow.js",
  "types": "fromnow.d.ts",
  "license": "MIT",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "author": {
    "name": "Luke Edwards",
    "email": "luke@lukeed.com",
    "url": "https://lukeed.com"
  },
  "scripts": {
    "build": "bundt",
    "pretest": "npm run build",
    "test": "TZ=America/Los_Angeles tape test/*.js | tap-spec"
  },
  "keywords": [
    "ago",
    "date",
    "time",
    "parse",
    "format",
    "moment",
    "from now",
    "fromnow"
  ],
  "devDependencies": {
    "bundt": "0.4.0",
    "tap-spec": "5.0.0",
    "tape": "4.11.0"
  }
}
